{
  "version": 2,
  "builds": [
    {
      "src": "vite.config.ts",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/[^.]+",
      "dest": "/dist/index.html",
      "status": 200
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ]
}